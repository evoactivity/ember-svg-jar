{{#if this.svgComponent}}
  <this.svgComponent
    @title={{@title}}
    @titleId={{this.titleId}}
    @desc={{@desc}}
    @descId={{this.descId}}
    role={{@role}}
    aria-label={{@ariaLabel}}
    aria-hidden={{this.isAriaHidden}}
    aria-labelledby={{this.ariaLabelledBy}}
    ...attributes
    {{did-update this.updateSvg @name}}
  />

{{/if}}