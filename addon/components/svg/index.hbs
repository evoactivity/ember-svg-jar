{{#if this._svgComponentName}}
  {{#let (component this._svgComponentName) as |SvgJar|}}
    <SvgJar
      {{did-update this.updateSvg @name}}
      @title={{@title}}
      @titleId={{this.titleId}}
      aria-label={{@ariaLabel}}
      aria-hidden={{this.isAriaHidden}}
      aria-labelledby={{this.titleId}}
      ...attributes
    />
  {{/let}}
{{/if}}